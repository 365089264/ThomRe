<?xml version="1.0" encoding="utf-8"?>
<settings>
  <source-db>
    <type>oracle</type>
    <conn>Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=10.35.63.143)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME = cmadb)));User Id=cne;Password=cne;Unicode=True;</conn>
  </source-db>
  <destination-db>
